# Ignore dependencies managed by Composer.
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/libraries/

# Ignore sensitive information
.env
.env.*
!.env.example
/web/sites/*/settings.php
/web/sites/*/settings.*.php

# Ignore Drupal's file storage directories
/private/files/
!/private/files/.htaccess
/web/sites/*/files/
!/web/sites/*/files/gin-custom.css

# Ignore default webcam directory
/web/webcam/
!/web/webcam/.htaccess

# Ignore files generated by IDEs or build tools
.idea/
.vscode/
.sass-cache/
node_modules/
.editorconfig
.gitattributes
phpcs.xml

# Ignore OS and webhost generated files
.DS_Store
._*
Thumbs.ini
desktop.ini
/web/.well-known/
/web/.php-ini
/web/.php-version

# Ignore the .htaccess to avoid noise from Hetzner konsoleH modifying this file.
/web/.htaccess

# .gitkeep files ensure the existance of a directory path itself in empty or ignored directories
!.gitkeep

# Ignore gtm dev and prod
config/sync/google_tag.container.GTM-*.yml
config/sync/google_tag.settings.yml
