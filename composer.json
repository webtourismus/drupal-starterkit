{
    "name": "webtourismus/drupal-starterkit",
    "description": "Opininated starterkit based on drupal/core-recommended:^10.1 - for internal use only.",
    "type": "metapackage",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "jjj/chosen": {
            "type": "package",
            "package": {
                "name": "jjj/chosen",
                "version": "2.2.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chosen"
                },
                "source": {
                    "url": "https://github.com/JJJ/chosen.git",
                    "type": "git",
                    "reference": "2.2.1"
                }
            }
        },
        "webtourismus/designsystem": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/designsystem.git"
        },
        "webtourismus/ebr": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/ebr.git"
        },
        "webtourismus/fa6": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/fa6.git"
        },
        "webtourismus/gin_custom": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/gin_custom.git"
        },
        "webtourismus/mailjet_iframe": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/mailjet_iframe.git"
        },
        "webtourismus/seasonal_paragraphs": {
            "type": "vcs",
            "url": "https://webtourismus@bitbucket.org/webtourismus/seasonal_paragraphs.git"
        }
    },
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "3.4.1",
        "drupal/allowed_formats": "^2.0",
        "drupal/amswap": "3.2.1",
        "drupal/antibot": "^2.0",
        "drupal/block_permissions": "^1.3",
        "drupal/choices": "^2.1.4",
        "drupal/chosen": "^4.0.0",
        "drupal/codemirror_editor": "^1.9",
        "drupal/components": "^3.0@beta",
        "drupal/config_entity_reference_selection": "^2.0",
        "drupal/cookies": "^1.2.3",
        "drupal/core-composer-scaffold": "^10.1.1",
        "drupal/core-recommended": "^10.1.1",
        "drupal/ctools": "^4.0.4",
        "drupal/entity_clone": "^2.0.0",
        "drupal/entity_usage": "^2.0.0",
        "drupal/features": "^5.0@dev",
        "drupal/field_config_cardinality": "^3.0.1",
        "drupal/field_formatter_range": "^1.4",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0.1",
        "drupal/geolocation": "^3.12",
        "drupal/gin": "^3.0",
        "drupal/gin_login": "^2.0.2",
        "drupal/gin_toolbar": "^1.0",
        "drupal/google_tag": "^2.0.2",
        "drupal/layout_paragraphs": "^2.0.3",
        "drupal/layout_paragraphs_limit": "^1.0.0",
        "drupal/link_attributes": "^1.14",
        "drupal/linkit": "^6.1.0",
        "drupal/media_entity_download": "^2.1",
        "drupal/media_library_form_element": "^2.0",
        "drupal/menu_admin_per_menu": "^1.5",
        "drupal/menu_item_extras": "^3.0.2",
        "drupal/metatag": "^2.0.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0.2",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/redirect": "^1.8",
        "drupal/simple_sitemap": "^4.1.6",
        "drupal/style_options": "^1.0.0",
        "drupal/svg_image_field": "^2.2",
        "drupal/tailwind_jit": "^1.0.1",
        "drupal/token": "^1.12",
        "drupal/token_filter": "^2.0",
        "drupal/translation_views": "^1.0@alpha",
        "drupal/twig_field": "^1.3",
        "drupal/twig_tweak": "^3.2.1",
        "drupal/view_mode_switch": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2.0",
        "drupal/webform_views": "^5.1",
        "drupal/weight": "^3.4",
        "drush/drush": "^11.4",
        "jjj/chosen": "^2.2",
        "vlucas/phpdotenv": "^5.5",
        "webtourismus/alpinejs": "^3",
        "webtourismus/alpinejs-collapse": "^3.12",
        "webtourismus/alpinejs-intersect": "^3.12",
        "webtourismus/alpinejs-persist": "^3.12",
        "webtourismus/aos": "^2.3",
        "webtourismus/designsystem": "^1.1.2",
        "webtourismus/ebr": "^1.1.1",
        "webtourismus/fa6": "^6.4",
        "webtourismus/flatpickr": "^4.6",
        "webtourismus/gin_custom": "^1.0.6",
        "webtourismus/glightbox": "^3.2.0",
        "webtourismus/lucide": "^0.216.0",
        "webtourismus/mailjet_iframe": "^1.0.0",
        "webtourismus/masonry": "^4.2.2",
        "webtourismus/seasonal_paragraphs": "^1.0.4",
        "webtourismus/splidejs": "^4.1.4",
        "webtourismus/tailwindcss-cli": "^3.3"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^10.1",
        "drupal/potx": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true
        },
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Issue 2546212 - Make field formatter settings translatable": "https://www.drupal.org/files/issues/2022-10-28/2546212-207.patch",
                "Issue 3004334 - Wide warning message for hidden untranslatable fields": "https://www.drupal.org/files/issues/2023-06-22/3004223_hide_untranslatable_fields_message_20230622.patch",
                "Issue 2685749 - Add a machine_name widget for string field types with a UniqueField constraint": "https://www.drupal.org/files/issues/2022-02-14/2685749-106.patch",
                "Issue 3345064 - Media library field_widget_id is not unique enough": "https://www.drupal.org/files/issues/2023-02-28/3345064.patch"
            },
            "drupal/config_entity_reference_selection": {
                "Issue 3313803 - Allow to togle between allow-list and block-list": "https://git.drupalcode.org/project/config_entity_reference_selection/-/merge_requests/8.patch"
            },
            "drupal/cookies": {
                "Issue 3348475 - Integration with Geolocation field module": "https://git.drupalcode.org/project/cookies/-/merge_requests/89.patch"
            },
            "drupal/field_group": {
                "Issue 3208592 - CSS class validation too strict": "https://git.drupalcode.org/project/field_group/-/merge_requests/33.patch"
            },
            "drupal/gin": {
                "Issue 3281984 - Secondary Toolbar breadcrumb shows edit label all node routes": "https://git.drupalcode.org/project/gin/-/merge_requests/177.patch",
                "Issue 3281343 - Use Gin's edit form for media enities": "https://www.drupal.org/files/issues/2023-04-21/edit-form-media-3281343-11.patch",
                "Issue 3270626 - Use Gin's edit form for taxonomy terms" : "./private/patches/gin/gin_edit_form_for_taxonomy_3270626_237.patch",
                "Issue 3375853 - WSOD when using icons in administrative lists": "https://git.drupalcode.org/project/gin/-/merge_requests/283/diffs.patch"
            },
            "drupal/gin_toolbar": {
                "Issue 3319445 - The breadcrumb shows Edit label when user has no permission": "https://www.drupal.org/files/issues/2022-11-07/3319445-edit-link-in-breadcrumb-region-no-permission-3.patch"
            },
            "drupal/layout_paragraphs": {
                "Issue 3371332 - Show layout categories in section UI": "./private/patches/layout_paragraphs/3371332_show_layout_categories_in_section_ui_20230629.patch",
                "Issue 3350158 - LP Builder doesn't set admin route context": "./private/patches/layout_paragraphs/3350168_lp_builder_does_not_set_admin_route_content_20230324.patch"
            },
            "drupal/link_attributes": {
                "Issue 2939514 - Add support for Linkit for Link field": "https://www.drupal.org/files/issues/2023-03-22/2939514-26_0.patch"
            },
            "drupal/migrate_plus": {
                "Issue 3040427 - Allow callback for Url source": "https://www.drupal.org/files/issues/2023-02-15/3040427-42-migrate_plus_multiple_urls.patch"
            },
            "drupal/style_options": {
                "Issue 3314947 - Leverage Drupal Form API for CSS class plugin": "https://git.drupalcode.org/project/style_options/-/merge_requests/5.patch"
            },
            "webtourismus/splidejs": {
                "Custom patch for Webtourismus Drupal admin and frontend theme": "./private/patches/splidejs/splidejs_patch_drupal_v4.1.4.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
